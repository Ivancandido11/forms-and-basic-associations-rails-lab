<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <br />
  </div>

  <div>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />
  </div>

  <div id="song_notes_1">
  <%= f.label :notes %>
  <input id="song_notes_1" name="song[note_contents][]" type="text">
  <br />
  </div>

  <div id="song_notes_2">
  <%= f.label :notes %>
  <input id="song_notes_2" name="song[note_contents][]" type="text">
  <br />
  </div>

  <%= f.collection_select :genre_id, Genre.all, :id, :name %>

  <%= f.submit %>
<% end %>


